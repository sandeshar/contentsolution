# SMTP settings for sending email (required for contact form notifications)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your_smtp_username
SMTP_PASS=your_smtp_password
FROM_EMAIL=no-reply@example.com
# Optional fallback admin email (if store settings contact email is not set)
ADMIN_EMAIL=admin@example.com
# Optional site title used in acknowledgment emails
NEXT_PUBLIC_SITE_TITLE=Your Company
NEXT_PUBLIC_SITE_DOMAIN=example.com
